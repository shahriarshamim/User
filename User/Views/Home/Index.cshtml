@{
    ViewData["Title"] = "Home Page";
}
<script>    
    $(document).ready(function(){
        $.get("https://dummyjson.com/users", function(data, status) {
            
            $.each(data.users, function(index,obj) {
                        var pdData = `<table class="table table-dark table-striped">
                                          <thead>
                                            <tr>          
                                              <th scope="col">Image</th>
                                              <th scope="col">Username</th>
                                              <th scope="col">Email</th>
                                              <th scope="col">Age</th>
                                            </tr>
                                          </thead>
                                          <tbody>
                                            <tr>          
                                              <td><img src="${obj.image}" class="img-fluid"></td>
                                              <td>${obj.username}</td>
                                              <td>${obj.email}</td>
                                              <td>${obj.age}</td>
                                            </tr>        
                                          </tbody>
                                        </table>`;
                $('#dvUsersRow').append(pdData);
            })
        });
    });   
</script>


<div id="dvUsers">
    <div id="dvUsersRow" class="row">

    </div>
</div>
